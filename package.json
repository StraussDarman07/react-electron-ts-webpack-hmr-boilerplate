{
  "name": "react-electron-ts-webpack-hmr-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate code, to use react with typescript and electron. It also has some real hot reload",
  "main": "main.js",
  "scripts": {
    "build:react": "webpack --mode development --config webpack.react.config.js",
    "build": "webpack --mode development",
    "start": "electron ./dist/main.js",
    "server": "npm run build && webpack-dev-server --config webpack.config.server.js",
    "devstart": "cross-env ELECTRON_DEV=dev electron ./dist/main.js"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "react-hot-loader",
    "hot-reloading",
    "hmr"
  ],
  "author": "Thomas Stachl",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/webpack-env": "^1.15.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "electron": "^8.1.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.20",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
